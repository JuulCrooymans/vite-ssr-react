  "dependencies": {
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.3.2",
    "express": "^4.18.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "vite": "^2.9.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-router": "^5.1.18",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
