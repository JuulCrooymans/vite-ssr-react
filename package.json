{
  "name": "vite-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev server.ts",
    "build:client": "vite build --outDir dist/client",
